{
  "name": "my-movies-collection",
  "scripts": {
    "dev": "nodemon server.js",
    "test": "NODE_ENV=test jest --root-dir server/tests --verbose --detectOpenHandles --colors 2>&1",
    "migrate:up": "knex migrate:up",
    "migrate:down": "knex migrate:down",
    "seed": "knex seed:run smallseed",
    "test:migrate:up": "knex --knexfile server/tests/knexfile.js migrate:up",
    "test:migrate:down": "knex --knexfile server/tests/knexfile.js migrate:down"
  },
  "nodemonConfig": {
    "ignore": [
      "public/**",
      "charts/**"
    ]
  },
  "dependencies": {
    "axios": "^1.3.3",
    "better-sqlite3": "^8.1.0",
    "cors": "^2.8.5",
    "express": ">=5.0.0-beta.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.21.2",
    "connect-livereload": "^0.6.1",
    "jest": "^29.4.3",
    "knex": "^2.4.2",
    "livereload": "^0.9.3",
    "nock": "^13.3.0",
    "nodemon": "^2.0.20"
  }
}
